#   the used tools
APXS=@APXS@

#   the default target
all: mod_suid.so

#   compile the DSO file
mod_suid.so: mod_suid.c
	$(APXS) -c mod_suid.c

#   install the DSO file into the Apache installation
#   and activate it in the Apache configuration
install: all
	@INSTALL_IT@

#   cleanup
clean:
	@rm -f mod_suid.o mod_suid.so

distclean: clean
	@rm -f config.h Makefile config.log config.status


